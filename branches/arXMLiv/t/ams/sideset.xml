<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" xml:id="S0.Ex1.m1" tex="\sideset{}{{}^{{\prime}}}{\sum}_{{i=1}}^{\infty}S_{i}" text="((sum ^ prime) _ (i = 1)) ^ infinity * S _ i">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMApp scriptpos="mid">
              <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
              <XMApp scriptpos="mid">
                <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                <XMApp scriptpos="mid">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                  <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                  <XMTok name="prime" role="SUPOP">′</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok role="UNKNOWN" font="italic">i</XMTok>
                  <XMTok meaning="1" role="NUMBER">1</XMTok>
                </XMApp>
              </XMApp>
              <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
              <XMTok role="UNKNOWN" font="italic">S</XMTok>
              <XMTok role="UNKNOWN" font="italic">i</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" xml:id="S0.Ex2.m1" tex="\sideset{}{}{\sum}x" text="sum@(x)">
        <XMath>
          <XMApp>
            <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
            <XMTok role="UNKNOWN" font="italic">x</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex3">
      <Math mode="display" xml:id="S0.Ex3.m1" tex="\sideset{{}_{a}^{b}}{{}_{c}^{d}}{\sum}x" text="(^ b _ a sum _ c) ^ d * x">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMApp scriptpos="mid">
              <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="pre2"/>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="pre2"/>
                    <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                    <XMTok role="UNKNOWN" font="italic">a</XMTok>
                  </XMApp>
                  <XMTok role="UNKNOWN" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="UNKNOWN" font="italic">d</XMTok>
            </XMApp>
            <XMTok role="UNKNOWN" font="italic">x</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex4">
      <Math mode="display" xml:id="S0.Ex4.m1" tex="\sideset{{}^{a}_{b}}{{}^{c}_{d}}{\sum}x" text="(_ b ^ a sum ^ c) _ d * x">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMApp scriptpos="mid">
              <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
              <XMApp>
                <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="pre2"/>
                  <XMApp>
                    <XMTok role="SUPERSCRIPTOP" scriptpos="pre2"/>
                    <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                    <XMTok role="UNKNOWN" font="italic">a</XMTok>
                  </XMApp>
                  <XMTok role="UNKNOWN" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="UNKNOWN" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="UNKNOWN" font="italic">d</XMTok>
            </XMApp>
            <XMTok role="UNKNOWN" font="italic">x</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex5">
      <Math mode="display" xml:id="S0.Ex5.m1" tex="\sideset{u}{{}_{e}v}{\sum}x" text="u * sum _ e * v * x">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok role="UNKNOWN" font="italic">u</XMTok>
            <XMApp scriptpos="mid">
              <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
              <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
              <XMTok role="UNKNOWN" font="italic">e</XMTok>
            </XMApp>
            <XMTok role="UNKNOWN" font="italic">v</XMTok>
            <XMTok role="UNKNOWN" font="italic">x</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
