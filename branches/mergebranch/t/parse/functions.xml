<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Basic Functions</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="g(a)=g(b)" text="g@(a) = g@(b)">
          <XMath xml:id="S1.Ex1.m1.1">
            <XMApp xml:id="S1.Ex1.m1.1.10">
              <XMTok meaning="equals" role="RELOP" xml:id="S1.Ex1.m1.1.5">=</XMTok>
              <XMApp xml:id="S1.Ex1.m1.1.10.1">
                <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex1.m1.1.1" font="italic">g</XMTok>
                <XMTok role="ID" xml:id="S1.Ex1.m1.1.3" font="italic">a</XMTok>
              </XMApp>
              <XMApp xml:id="S1.Ex1.m1.1.10.2">
                <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex1.m1.1.6" font="italic">g</XMTok>
                <XMTok role="ID" xml:id="S1.Ex1.m1.1.8" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="f(a)=g(h(c))" text="f@(a) = g@(h@(c))">
          <XMath xml:id="S1.Ex2.m1.1">
            <XMApp xml:id="S1.Ex2.m1.1.13">
              <XMTok meaning="equals" role="RELOP" xml:id="S1.Ex2.m1.1.5">=</XMTok>
              <XMApp xml:id="S1.Ex2.m1.1.13.1">
                <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex2.m1.1.1" font="italic">f</XMTok>
                <XMTok role="ID" xml:id="S1.Ex2.m1.1.3" font="italic">a</XMTok>
              </XMApp>
              <XMApp xml:id="S1.Ex2.m1.1.13.2">
                <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex2.m1.1.6" font="italic">g</XMTok>
                <XMApp xml:id="S1.Ex2.m1.1.13.2.1">
                  <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex2.m1.1.8" font="italic">h</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex2.m1.1.10" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" xml:id="S1.Ex3.m1" tex="h(c)=\sin a^{2}" text="h@(c) = sine@(a ^ 2)">
          <XMath xml:id="S1.Ex3.m1.1">
            <XMApp xml:id="S1.Ex3.m1.1.9">
              <XMTok meaning="equals" role="RELOP" xml:id="S1.Ex3.m1.1.5">=</XMTok>
              <XMApp xml:id="S1.Ex3.m1.1.9.1">
                <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex3.m1.1.1" font="italic">h</XMTok>
                <XMTok role="ID" xml:id="S1.Ex3.m1.1.3" font="italic">c</XMTok>
              </XMApp>
              <XMApp xml:id="S1.Ex3.m1.1.9.2">
                <XMTok meaning="sine" role="TRIGFUNCTION" xml:id="S1.Ex3.m1.1.6">sin</XMTok>
                <XMApp xml:id="S1.Ex3.m1.1.9.2.1">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S1.Ex3.m1.1.9.2.1.1"/>
                  <XMTok role="ID" xml:id="S1.Ex3.m1.1.7" font="italic">a</XMTok>
                  <XMTok meaning="2" role="NUMBER" xml:id="S1.Ex3.m1.1.8.1">2</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" xml:id="S1.Ex4.m1" tex="\sin f(a)" text="sine@(f@(a))">
          <XMath xml:id="S1.Ex4.m1.1">
            <XMApp xml:id="S1.Ex4.m1.1.6">
              <XMTok meaning="sine" role="TRIGFUNCTION" xml:id="S1.Ex4.m1.1.1">sin</XMTok>
              <XMApp xml:id="S1.Ex4.m1.1.6.1">
                <XMTok argclose=")" argopen="(" role="FUNCTION" xml:id="S1.Ex4.m1.1.2" font="italic">f</XMTok>
                <XMTok role="ID" xml:id="S1.Ex4.m1.1.4" font="italic">a</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Trigonometric functions</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex5">
        <Math mode="display" xml:id="S2.Ex5.m1" tex="\sin a+\tan b" text="sine@(a) + tangent@(b)">
          <XMath xml:id="S2.Ex5.m1.1">
            <XMApp xml:id="S2.Ex5.m1.1.6">
              <XMTok meaning="plus" role="ADDOP" xml:id="S2.Ex5.m1.1.3">+</XMTok>
              <XMApp xml:id="S2.Ex5.m1.1.6.1">
                <XMTok meaning="sine" role="TRIGFUNCTION" xml:id="S2.Ex5.m1.1.1">sin</XMTok>
                <XMTok role="ID" xml:id="S2.Ex5.m1.1.2" font="italic">a</XMTok>
              </XMApp>
              <XMApp xml:id="S2.Ex5.m1.1.6.2">
                <XMTok meaning="tangent" role="TRIGFUNCTION" xml:id="S2.Ex5.m1.1.4">tan</XMTok>
                <XMTok role="ID" xml:id="S2.Ex5.m1.1.5" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex6">
        <Math mode="display" xml:id="S2.Ex6.m1" tex="\sin a\cos b\neq\sin(a\cos b)" text="sine@(a) * cosine@(b) not-equals sine@(a * cosine@(b))">
          <XMath xml:id="S2.Ex6.m1.1">
            <XMApp xml:id="S2.Ex6.m1.1.12">
              <XMTok meaning="not-equals" name="neq" role="RELOP" xml:id="S2.Ex6.m1.1.5">≠</XMTok>
              <XMApp xml:id="S2.Ex6.m1.1.12.1">
                <XMTok meaning="times" role="MULOP" xml:id="S2.Ex6.m1.1.12.1.1">⁢</XMTok>
                <XMApp xml:id="S2.Ex6.m1.1.12.1.2">
                  <XMTok meaning="sine" role="TRIGFUNCTION" xml:id="S2.Ex6.m1.1.1">sin</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex6.m1.1.2" font="italic">a</XMTok>
                </XMApp>
                <XMApp xml:id="S2.Ex6.m1.1.12.1.3">
                  <XMTok meaning="cosine" role="TRIGFUNCTION" xml:id="S2.Ex6.m1.1.3">cos</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex6.m1.1.4" font="italic">b</XMTok>
                </XMApp>
              </XMApp>
              <XMApp xml:id="S2.Ex6.m1.1.12.2">
                <XMTok argclose=")" argopen="(" meaning="sine" role="TRIGFUNCTION" xml:id="S2.Ex6.m1.1.6">sin</XMTok>
                <XMApp xml:id="S2.Ex6.m1.1.12.2.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S2.Ex6.m1.1.12.2.1.1">⁢</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex6.m1.1.8" font="italic">a</XMTok>
                  <XMApp xml:id="S2.Ex6.m1.1.12.2.1.2">
                    <XMTok meaning="cosine" role="TRIGFUNCTION" xml:id="S2.Ex6.m1.1.9">cos</XMTok>
                    <XMTok role="ID" xml:id="S2.Ex6.m1.1.10" font="italic">b</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex7">
        <Math mode="display" xml:id="S2.Ex7.m1" tex="\sin 2\pi\neq\log 2x^{2}" text="sine@(2 * pi) not-equals logarithm@(2 * x ^ 2)">
          <XMath xml:id="S2.Ex7.m1.1">
            <XMApp xml:id="S2.Ex7.m1.1.9">
              <XMTok meaning="not-equals" name="neq" role="RELOP" xml:id="S2.Ex7.m1.1.4">≠</XMTok>
              <XMApp xml:id="S2.Ex7.m1.1.9.1">
                <XMTok meaning="sine" role="TRIGFUNCTION" xml:id="S2.Ex7.m1.1.1">sin</XMTok>
                <XMApp xml:id="S2.Ex7.m1.1.9.1.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S2.Ex7.m1.1.9.1.1.1">⁢</XMTok>
                  <XMTok meaning="2" role="NUMBER" xml:id="S2.Ex7.m1.1.2">2</XMTok>
                  <XMTok name="pi" role="UNKNOWN" xml:id="S2.Ex7.m1.1.3" font="italic">π</XMTok>
                </XMApp>
              </XMApp>
              <XMApp xml:id="S2.Ex7.m1.1.9.2">
                <XMTok meaning="logarithm" role="OPFUNCTION" xml:id="S2.Ex7.m1.1.5">log</XMTok>
                <XMApp xml:id="S2.Ex7.m1.1.9.2.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S2.Ex7.m1.1.9.2.1.1">⁢</XMTok>
                  <XMTok meaning="2" role="NUMBER" xml:id="S2.Ex7.m1.1.6">2</XMTok>
                  <XMApp xml:id="S2.Ex7.m1.1.9.2.1.2">
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S2.Ex7.m1.1.9.2.1.2.1"/>
                    <XMTok role="UNKNOWN" xml:id="S2.Ex7.m1.1.7" font="italic">x</XMTok>
                    <XMTok meaning="2" role="NUMBER" xml:id="S2.Ex7.m1.1.8.1">2</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag close=" ">3</tag>Missing arguments</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex8">
        <Math mode="display" xml:id="S3.Ex8.m1" tex="fgh" text="f * g * h">
          <XMath xml:id="S3.Ex8.m1.1">
            <XMApp xml:id="S3.Ex8.m1.1.4">
              <XMTok meaning="times" role="MULOP" xml:id="S3.Ex8.m1.1.4.1">⁢</XMTok>
              <XMTok role="FUNCTION" xml:id="S3.Ex8.m1.1.1" font="italic">f</XMTok>
              <XMTok role="FUNCTION" xml:id="S3.Ex8.m1.1.2" font="italic">g</XMTok>
              <XMTok role="FUNCTION" xml:id="S3.Ex8.m1.1.3" font="italic">h</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex9">
        <Math mode="display" xml:id="S3.Ex9.m1" tex="fgha" text="f * g * h * a">
          <XMath xml:id="S3.Ex9.m1.1">
            <XMApp xml:id="S3.Ex9.m1.1.5">
              <XMTok meaning="times" role="MULOP" xml:id="S3.Ex9.m1.1.5.1">⁢</XMTok>
              <XMTok role="FUNCTION" xml:id="S3.Ex9.m1.1.1" font="italic">f</XMTok>
              <XMTok role="FUNCTION" xml:id="S3.Ex9.m1.1.2" font="italic">g</XMTok>
              <XMTok role="FUNCTION" xml:id="S3.Ex9.m1.1.3" font="italic">h</XMTok>
              <XMTok role="ID" xml:id="S3.Ex9.m1.1.4" font="italic">a</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag close=" ">4</tag>Operator-like functions</title>
    <para xml:id="S4.p1">
      <equation xml:id="S4.Ex10">
        <Math mode="display" xml:id="S4.Ex10.m1" tex="FGH" text="F@(G@(H))">
          <XMath xml:id="S4.Ex10.m1.1">
            <XMApp xml:id="S4.Ex10.m1.1.4">
              <XMTok role="OPFUNCTION" xml:id="S4.Ex10.m1.1.1" font="italic">F</XMTok>
              <XMApp xml:id="S4.Ex10.m1.1.4.1">
                <XMTok role="OPFUNCTION" xml:id="S4.Ex10.m1.1.2" font="italic">G</XMTok>
                <XMTok role="OPFUNCTION" xml:id="S4.Ex10.m1.1.3" font="italic">H</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S4.Ex11">
        <Math mode="display" xml:id="S4.Ex11.m1" tex="FGHa" text="F@(G@(H@(a)))">
          <XMath xml:id="S4.Ex11.m1.1">
            <XMApp xml:id="S4.Ex11.m1.1.5">
              <XMTok role="OPFUNCTION" xml:id="S4.Ex11.m1.1.1" font="italic">F</XMTok>
              <XMApp xml:id="S4.Ex11.m1.1.5.1">
                <XMTok role="OPFUNCTION" xml:id="S4.Ex11.m1.1.2" font="italic">G</XMTok>
                <XMApp xml:id="S4.Ex11.m1.1.5.1.1">
                  <XMTok role="OPFUNCTION" xml:id="S4.Ex11.m1.1.3" font="italic">H</XMTok>
                  <XMTok role="ID" xml:id="S4.Ex11.m1.1.4" font="italic">a</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S4.p2">
      <equation xml:id="S4.Ex12">
        <Math mode="display" xml:id="S4.Ex12.m1" tex="f^{{\prime}}(a)=a^{2}" text="(f ^ prime)@(a) = a ^ 2">
          <XMath xml:id="S4.Ex12.m1.1">
            <XMApp xml:id="S4.Ex12.m1.1.9">
              <XMTok meaning="equals" role="RELOP" xml:id="S4.Ex12.m1.1.6">=</XMTok>
              <XMApp xml:id="S4.Ex12.m1.1.9.1">
                <XMApp argclose=")" argopen="(" xml:id="S4.Ex12.m1.1.9.1.1">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S4.Ex12.m1.1.9.1.1.1"/>
                  <XMTok role="FUNCTION" xml:id="S4.Ex12.m1.1.1" font="italic">f</XMTok>
                  <XMTok name="prime" role="SUPOP" xml:id="S4.Ex12.m1.1.2.1">′</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S4.Ex12.m1.1.4" font="italic">a</XMTok>
              </XMApp>
              <XMApp xml:id="S4.Ex12.m1.1.9.2">
                <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S4.Ex12.m1.1.9.2.1"/>
                <XMTok role="ID" xml:id="S4.Ex12.m1.1.7" font="italic">a</XMTok>
                <XMTok meaning="2" role="NUMBER" xml:id="S4.Ex12.m1.1.8.1">2</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S4.Ex13">
        <Math mode="display" xml:id="S4.Ex13.m1" tex="f^{2}(a)=g^{b}(2)" text="(f ^ 2)@(a) = (g ^ b)@(2)">
          <XMath xml:id="S4.Ex13.m1.1">
            <XMApp xml:id="S4.Ex13.m1.1.12">
              <XMTok meaning="equals" role="RELOP" xml:id="S4.Ex13.m1.1.6">=</XMTok>
              <XMApp xml:id="S4.Ex13.m1.1.12.1">
                <XMApp argclose=")" argopen="(" xml:id="S4.Ex13.m1.1.12.1.1">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S4.Ex13.m1.1.12.1.1.1"/>
                  <XMTok role="FUNCTION" xml:id="S4.Ex13.m1.1.1" font="italic">f</XMTok>
                  <XMTok meaning="2" role="NUMBER" xml:id="S4.Ex13.m1.1.2.1">2</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S4.Ex13.m1.1.4" font="italic">a</XMTok>
              </XMApp>
              <XMApp xml:id="S4.Ex13.m1.1.12.2">
                <XMApp argclose=")" argopen="(" xml:id="S4.Ex13.m1.1.12.2.1">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S4.Ex13.m1.1.12.2.1.1"/>
                  <XMTok role="FUNCTION" xml:id="S4.Ex13.m1.1.7" font="italic">g</XMTok>
                  <XMTok role="ID" xml:id="S4.Ex13.m1.1.8.1" font="italic">b</XMTok>
                </XMApp>
                <XMTok meaning="2" role="NUMBER" xml:id="S4.Ex13.m1.1.10">2</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="5" xml:id="S5">
    <title><tag close=" ">5</tag>Scripts</title>
    <para xml:id="S5.p1">
      <equation xml:id="S5.Ex14">
        <Math mode="display" xml:id="S5.Ex14.m1" tex="\ln a=\log_{e}a=\ln_{{+}}b" text="natural-logarithm@(a) = (logarithm _ e)@(a) = (natural-logarithm _ +)@(b)">
          <XMath xml:id="S5.Ex14.m1.1">
            <XMApp xml:id="S5.Ex14.m1.1.11">
              <XMTok meaning="multirelation" xml:id="S5.Ex14.m1.1.11.1"/>
              <XMApp xml:id="S5.Ex14.m1.1.11.2">
                <XMTok meaning="natural-logarithm" role="OPFUNCTION" xml:id="S5.Ex14.m1.1.1">ln</XMTok>
                <XMTok role="ID" xml:id="S5.Ex14.m1.1.2" font="italic">a</XMTok>
              </XMApp>
              <XMTok meaning="equals" role="RELOP" xml:id="S5.Ex14.m1.1.3">=</XMTok>
              <XMApp xml:id="S5.Ex14.m1.1.11.3">
                <XMApp xml:id="S5.Ex14.m1.1.11.3.1">
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2" xml:id="S5.Ex14.m1.1.11.3.1.1"/>
                  <XMTok meaning="logarithm" role="OPFUNCTION" xml:id="S5.Ex14.m1.1.4">log</XMTok>
                  <XMTok role="ID" xml:id="S5.Ex14.m1.1.5.1" font="italic">e</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S5.Ex14.m1.1.6" font="italic">a</XMTok>
              </XMApp>
              <XMTok meaning="equals" role="RELOP" xml:id="S5.Ex14.m1.1.7">=</XMTok>
              <XMApp xml:id="S5.Ex14.m1.1.11.4">
                <XMApp xml:id="S5.Ex14.m1.1.11.4.1">
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2" xml:id="S5.Ex14.m1.1.11.4.1.1"/>
                  <XMTok meaning="natural-logarithm" role="OPFUNCTION" xml:id="S5.Ex14.m1.1.8">ln</XMTok>
                  <XMTok meaning="plus" role="ADDOP" xml:id="S5.Ex14.m1.1.9.1">+</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S5.Ex14.m1.1.10" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
