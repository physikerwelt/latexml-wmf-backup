<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="url"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Basic URLS</title>
    <para xml:id="S1.p1">
      <p xml:id="S1.p1.1">Basic url: <ref class="ltx_url" href="http://example.com/~user"><text font="typewriter">http://example.com/~user</text></ref> or <ref class="ltx_url" href="http://example.com/~user"><text font="typewriter">http://example.com/~user</text></ref>.</p>
    </para>
    <para xml:id="S1.p2">
      <p xml:id="S1.p2.1">And even: <ref class="ltx_url" href="http://example.com/~user"><text font="typewriter">http://example.com/\~{}user</text></ref> or <ref class="ltx_url" href="http://example.com/~user"><text font="typewriter">http://example.com/\~{}user</text></ref>.</p>
    </para>
    <para xml:id="S1.p3">
      <p xml:id="S1.p3.1">Path url: <ref class="ltx_path" href="/foo/bar/baz"><text font="typewriter">/foo/bar/baz</text></ref> or <ref class="ltx_path" href="/foo/bar/baz"><text font="typewriter">/foo/bar/baz</text></ref></p>
    </para>
    <para xml:id="S1.p4">
      <p xml:id="S1.p4.1">Verbatimness <ref class="ltx_path" href="C:\foo\bar\baz"><text font="typewriter">C:\foo\bar\baz</text></ref> or <ref class="ltx_path" href="C:\foo\bar\baz"><text font="typewriter">C:\foo\bar\baz</text></ref></p>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Styles</title>
    <para xml:id="S2.p1">
      <p xml:id="S2.p1.1">Email: <ref class="ltx_email" href="myself%node@gateway.net"><text font="sansserif">myself%node@gateway.net</text></ref> or  <ref class="ltx_email" href="myself%node@gateway.net"><text font="sansserif">myself%node@gateway.net</text></ref>.</p>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag close=" ">3</tag>Defined urls</title>
    <para xml:id="S3.p1">
      <p xml:id="S3.p1.1">Myself: <ref class="ltx_url" href="myself%node@gateway.net"><text font="typewriter">myself%node@gateway.net</text></ref> or <ref class="ltx_email" href="myself%node@gateway.net"><text font="sansserif">myself%node@gateway.net</text></ref>.</p>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag close=" ">4</tag>Bracketting</title>
    <para xml:id="S4.p1">
      <p xml:id="S4.p1.1">Fancy url: <ref class="ltx_fancyurl" href="http://example.com/~user"><text font="typewriter">&lt;url: http://example.com/~user&gt;</text></ref> or <ref class="ltx_fancyurl" href="http://example.com/~user"><text font="typewriter">&lt;url: http://example.com/~user&gt;</text></ref>.</p>
    </para>
  </section>
</document>
