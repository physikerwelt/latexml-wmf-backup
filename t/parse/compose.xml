<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag>1 </tag>Compositions</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="(f*g)(x)" text="compose@(f, g) * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                <XMTok role="FUNCTION" font="italic">f</XMTok>
                <XMTok role="FUNCTION" font="italic">g</XMTok>
              </XMApp>
              <XMTok close=")" open="(" role="ID" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="(f*g*h)(x)" text="compose@(compose@(f, g), h) * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                <XMApp>
                  <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                  <XMTok role="FUNCTION" font="italic">f</XMTok>
                  <XMTok role="FUNCTION" font="italic">g</XMTok>
                </XMApp>
                <XMTok role="FUNCTION" font="italic">h</XMTok>
              </XMApp>
              <XMTok close=")" open="(" role="ID" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag>2 </tag>Implicit lambdas?</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex3">
        <Math mode="display" xml:id="S2.Ex3.m1" tex="(y^{2}*f)(x)" text="compose@(y ^ 2, f) * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                  <XMTok role="ID" font="italic">y</XMTok>
                  <XMTok meaning="2" role="NUMBER">2</XMTok>
                </XMApp>
                <XMTok role="FUNCTION" font="italic">f</XMTok>
              </XMApp>
              <XMTok close=")" open="(" role="ID" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex4">
        <Math mode="display" xml:id="S2.Ex4.m1" tex="(yy^{2}*f)(x)" text="compose@(y * y ^ 2, f) * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok role="ID" font="italic">y</XMTok>
                  <XMApp>
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                    <XMTok role="ID" font="italic">y</XMTok>
                    <XMTok meaning="2" role="NUMBER">2</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok role="FUNCTION" font="italic">f</XMTok>
              </XMApp>
              <XMTok close=")" open="(" role="ID" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex5">
        <Math mode="display" xml:id="S2.Ex5.m1" tex="(y^{2}*f)(x)" text="compose@(y ^ 2, f) * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                  <XMTok role="ID" font="italic">y</XMTok>
                  <XMTok meaning="2" role="NUMBER">2</XMTok>
                </XMApp>
                <XMTok role="FUNCTION" font="italic">f</XMTok>
              </XMApp>
              <XMTok close=")" open="(" role="ID" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex6">
        <Math mode="display" xml:id="S2.Ex6.m1" tex="(f*y^{2})(x)" text="compose@(f, y ^ 2) * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="compose" role="COMPOSEOP">*</XMTok>
                <XMTok role="FUNCTION" font="italic">f</XMTok>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                  <XMTok role="ID" font="italic">y</XMTok>
                  <XMTok meaning="2" role="NUMBER">2</XMTok>
                </XMApp>
              </XMApp>
              <XMTok close=")" open="(" role="ID" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
