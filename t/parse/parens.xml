<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag>1 </tag>Basic math</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="((a+b)-c)" text="(a + b) - c">
          <XMath>
            <XMApp close=")" open="(">
              <XMTok meaning="minus" role="ADDOP">-</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">c</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="(a+(b-c))" text="a + (b - c)">
          <XMath>
            <XMApp close=")" open="(">
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp close=")" open="(">
                <XMTok meaning="minus" role="ADDOP">-</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag>2 </tag>Intervals</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex3">
        <Math mode="display" xml:id="S2.Ex3.m1" tex="(a,b)" text="open-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok argclose=")" argopen="(" meaning="open-interval" role="FENCED" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex4">
        <Math mode="display" xml:id="S2.Ex4.m1" tex="[a,b)" text="closed-open-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok argclose=")" argopen="[" meaning="closed-open-interval" role="FENCED" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex5">
        <Math mode="display" xml:id="S2.Ex5.m1" tex="(a,b]" text="open-closed-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok argclose="]" argopen="(" meaning="open-closed-interval" role="FENCED" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex6">
        <Math mode="display" xml:id="S2.Ex6.m1" tex="[a,b]" text="closed-interval@(a, b)">
          <XMath>
            <XMApp>
              <XMTok argclose="]" argopen="[" meaning="closed-interval" role="FENCED" separators=","/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag>3 </tag>Cases</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex7">
        <Math mode="display" xml:id="S3.Ex7.m1" tex="\left.\begin{array}[]{l}a\\&#10;b\end{array}\right\}=\left\{\begin{array}[]{l}c\\&#10;d\end{array}\right." text="cases[[a], [b]] = cases[[c], [d]]">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMArray close="}" meaning="cases" role="ARRAY">
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">a</XMTok>
                  </XMCell>
                </XMRow>
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">b</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
              <XMArray meaning="cases" open="{" role="ARRAY">
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">c</XMTok>
                  </XMCell>
                </XMRow>
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">d</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S3.p2">
      <equation xml:id="S3.Ex8">
        <Math mode="display" xml:id="S3.Ex8.m1" tex="\left(\begin{array}[]{l}c\\&#10;d\end{array}\right)" text="Array[[c], [d]]">
          <XMath>
            <XMArray close=")" open="(" role="ARRAY">
              <XMRow>
                <XMCell align="left">
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMCell>
              </XMRow>
              <XMRow>
                <XMCell align="left">
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMCell>
              </XMRow>
            </XMArray>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex9">
        <Math mode="display" xml:id="S3.Ex9.m1" tex="(a+\left(\begin{array}[]{l}c\\&#10;d\end{array}\right)+b)" text="a + Array[[c], [d]] + b">
          <XMath>
            <XMApp close=")" open="(">
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMArray close=")" open="(" role="ARRAY">
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">c</XMTok>
                  </XMCell>
                </XMRow>
                <XMRow>
                  <XMCell align="left">
                    <XMTok role="ID" font="italic">d</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag>4 </tag>Atop</title>
    <para xml:id="S4.p1">
      <equation xml:id="S4.Ex10">
        <Math mode="display" xml:id="S4.Ex10.m1" tex="a+{b\atop c}+d" text="a + STACKED@(b, c) + d">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok mathstyle="display" role="STACKED"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">d</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S4.Ex11">
        <Math mode="display" xml:id="S4.Ex11.m1" tex="a+\left[b\atop c\right]+d" text="a + STACKED@(b, c) + d">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp close="]" open="[">
                <XMTok mathstyle="display" role="STACKED"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">d</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="5" xml:id="S5">
    <title><tag>5 </tag>Delimiters</title>
    <para xml:id="S5.p1">
      <equation xml:id="S5.Ex12">
        <Math mode="display" xml:id="S5.Ex12.m1" tex="\left({a\over b}\right)" text="a / b">
          <XMath>
            <XMApp close=")" open="(">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex13">
        <Math mode="display" xml:id="S5.Ex13.m1" tex="\left[{a\over b}\right]" text="a / b">
          <XMath>
            <XMApp close="]" open="[">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex14">
        <Math mode="display" xml:id="S5.Ex14.m1" tex="\left[{a\over b}\right]" text="a / b">
          <XMath>
            <XMApp close="]" open="[">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex15">
        <Math mode="display" xml:id="S5.Ex15.m1" tex="\left\{{a\over b}\right\}" text="set@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="}" argopen="{" meaning="set" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex16">
        <Math mode="display" xml:id="S5.Ex16.m1" tex="\left\{{a\over b}\right\}" text="set@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="}" argopen="{" meaning="set" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex17">
        <Math mode="display" xml:id="S5.Ex17.m1" tex="\left\lfloor{a\over b}\right\rfloor" text="floor@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="⌋" argopen="⌊" meaning="floor" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex18">
        <Math mode="display" xml:id="S5.Ex18.m1" tex="\left\lceil{a\over b}\right\rceil" text="ceiling@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="⌉" argopen="⌈" meaning="ceiling" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex19">
        <Math mode="display" xml:id="S5.Ex19.m1" tex="\left&lt;{a\over b}\right&gt;" text="a / b">
          <XMath>
            <XMApp close="⟩" open="⟨">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex20">
        <Math mode="display" xml:id="S5.Ex20.m1" tex="\left\langle{a\over b}\right\rangle" text="a / b">
          <XMath>
            <XMApp close="⟩" open="⟨">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex21">
        <Math mode="display" xml:id="S5.Ex21.m1" tex="\left|{a\over b}\right|" text="absolute-value@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex22">
        <Math mode="display" xml:id="S5.Ex22.m1" tex="\left|{a\over b}\right|" text="absolute-value@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex23">
        <Math mode="display" xml:id="S5.Ex23.m1" tex="\left\|{a\over b}\right\|" text="norm@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="∥" argopen="∥" meaning="norm" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex24">
        <Math mode="display" xml:id="S5.Ex24.m1" tex="\left\|{a\over b}\right\|" text="norm@(a / b)">
          <XMath>
            <XMApp>
              <XMTok argclose="∥" argopen="∥" meaning="norm" role="FENCED"/>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex25">
        <Math mode="display" xml:id="S5.Ex25.m1" tex="\left\uparrow{a\over b}\right\downarrow" text="a / b">
          <XMath>
            <XMApp close="↓" open="↑">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex26">
        <Math mode="display" xml:id="S5.Ex26.m1" tex="\left\Uparrow{a\over b}\right\Downarrow" text="a / b">
          <XMath>
            <XMApp close="⇓" open="⇑">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S5.Ex27">
        <Math mode="display" xml:id="S5.Ex27.m1" tex="\left\updownarrow{a\over b}\right\Updownarrow" text="a / b">
          <XMath>
            <XMApp close="⇕" open="↕">
              <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S5.p2">
      <equation xml:id="S5.Ex28">
        <Math mode="display" xml:id="S5.Ex28.m1" tex="a+\left\langle{b\over c}\right\rangle+d" text="a + b / c + d">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp close="⟩" open="⟨">
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
              </XMApp>
              <XMTok role="ID" font="italic">d</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S5.p3">
      <equation xml:id="S5.Ex29">
        <Math mode="display" xml:id="S5.Ex29.m1" tex="{a\over b}\left/{c\over d}\right." text="(a / b) / (c / d)">
          <XMath>
            <XMApp>
              <XMTok role="MULOP">/</XMTok>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="MULOP"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
