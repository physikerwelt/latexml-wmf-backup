  <resource src="ltx-article.css" type="text/css"/>
  <title>Less Than, Greater Than, Bars and Quantum Mechanics</title>
  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Relations</title>
    <para xml:id="S1.p1">
      <p xml:id="S1.p1.1">Relations: <Math mode="inline" xml:id="S1.p1.1.m1" tex="a&lt;b&lt;c" text="a less b less c"><XMath xml:id="S1.p1.1.m1.1"><XMApp xml:id="S1.p1.1.m1.1.6"><XMTok meaning="multirelation" xml:id="S1.p1.1.m1.1.6.1"/><XMTok role="UNKNOWN" xml:id="S1.p1.1.m1.1.1" font="italic">a</XMTok><XMTok meaning="less-than" role="RELOP" xml:id="S1.p1.1.m1.1.2">&lt;</XMTok><XMTok role="UNKNOWN" xml:id="S1.p1.1.m1.1.3" font="italic">b</XMTok><XMTok meaning="less-than" role="RELOP" xml:id="S1.p1.1.m1.1.4">&lt;</XMTok><XMTok role="UNKNOWN" xml:id="S1.p1.1.m1.1.5" font="italic">c</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S1.p2">
      <p xml:id="S1.p2.1">Relations: <Math mode="inline" xml:id="S1.p2.1.m1" tex="&gt;a" text="absent &gt; a"><XMath xml:id="S1.p2.1.m1.1"><XMApp xml:id="S1.p2.1.m1.1.3"><XMTok meaning="greater-than" role="RELOP" xml:id="S1.p2.1.m1.1.1">&gt;</XMTok><XMTok meaning="absent" xml:id="S1.p2.1.m1.1.3.1"/><XMTok role="UNKNOWN" xml:id="S1.p2.1.m1.1.2" font="italic">a</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S1.p3">
      <p xml:id="S1.p3.1">Relations: <Math mode="inline" xml:id="S1.p3.1.m1" tex="a&gt;" text="a &gt; absent"><XMath xml:id="S1.p3.1.m1.1"><XMApp xml:id="S1.p3.1.m1.1.3"><XMTok meaning="greater-than" role="RELOP" xml:id="S1.p3.1.m1.1.2">&gt;</XMTok><XMTok role="UNKNOWN" xml:id="S1.p3.1.m1.1.1" font="italic">a</XMTok><XMTok meaning="absent" xml:id="S1.p3.1.m1.1.3.1"/></XMApp></XMath></Math></p>
    </para>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Quantum Mechanics Notations</title>
    <para xml:id="S2.p1">
      <p xml:id="S2.p1.1">Expectation: <Math mode="inline" xml:id="S2.p1.1.m1" tex="&lt;a&gt;" text="expectation@(a)"><XMath xml:id="S2.p1.1.m1.1"><XMApp xml:id="S2.p1.1.m1.1.4"><XMTok argclose="&gt;" argopen="&lt;" meaning="expectation" role="FENCED" xml:id="S2.p1.1.m1.1.4.1"/><XMTok role="UNKNOWN" xml:id="S2.p1.1.m1.1.2" font="italic">a</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S2.p2">
      <p xml:id="S2.p2.1">Bra: <Math mode="inline" xml:id="S2.p2.1.m1" tex="&lt;a|" text="bra@(a)"><XMath xml:id="S2.p2.1.m1.1"><XMApp xml:id="S2.p2.1.m1.1.4"><XMTok argclose="|" argopen="&lt;" meaning="bra" role="FENCED" xml:id="S2.p2.1.m1.1.4.1"/><XMTok role="UNKNOWN" xml:id="S2.p2.1.m1.1.2" font="italic">a</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S2.p3">
      <p xml:id="S2.p3.1">Bra: <Math mode="inline" xml:id="S2.p3.1.m1" tex="&lt;a,b,c|" text="bra@(list@(a, b, c))"><XMath xml:id="S2.p3.1.m1.1"><XMApp xml:id="S2.p3.1.m1.1.8"><XMTok argclose="|" argopen="&lt;" meaning="bra" role="FENCED" xml:id="S2.p3.1.m1.1.8.1"/><XMApp xml:id="S2.p3.1.m1.1.8.2"><XMTok meaning="list" role="FENCED" separators=", ," xml:id="S2.p3.1.m1.1.8.2.1"/><XMTok role="UNKNOWN" xml:id="S2.p3.1.m1.1.2" font="italic">a</XMTok><XMTok role="UNKNOWN" xml:id="S2.p3.1.m1.1.4" font="italic">b</XMTok><XMTok role="UNKNOWN" xml:id="S2.p3.1.m1.1.6" font="italic">c</XMTok></XMApp></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S2.p4">
      <p xml:id="S2.p4.1">Bra: <Math mode="inline" xml:id="S2.p4.1.m1" tex="&lt;\phi=0|" text="bra@(phi = 0)"><XMath xml:id="S2.p4.1.m1.1"><XMApp xml:id="S2.p4.1.m1.1.6"><XMTok argclose="|" argopen="&lt;" meaning="bra" role="FENCED" xml:id="S2.p4.1.m1.1.6.1"/><XMApp xml:id="S2.p4.1.m1.1.6.2"><XMTok meaning="equals" role="RELOP" xml:id="S2.p4.1.m1.1.3">=</XMTok><XMTok name="phi" role="UNKNOWN" xml:id="S2.p4.1.m1.1.2" font="italic">ϕ</XMTok><XMTok meaning="0" role="NUMBER" xml:id="S2.p4.1.m1.1.4">0</XMTok></XMApp></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S2.p5">
      <p xml:id="S2.p5.1">Ket: <Math mode="inline" xml:id="S2.p5.1.m1" tex="|b&gt;" text="ket@(b)"><XMath xml:id="S2.p5.1.m1.1"><XMApp xml:id="S2.p5.1.m1.1.4"><XMTok argclose="&gt;" argopen="|" meaning="ket" role="FENCED" xml:id="S2.p5.1.m1.1.4.1"/><XMTok role="UNKNOWN" xml:id="S2.p5.1.m1.1.2" font="italic">b</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S2.p6">
      <p xml:id="S2.p6.1">Braket <Math mode="inline" xml:id="S2.p6.1.m1" tex="&lt;a|b&gt;" text="inner-product@(bra@(a), ket@(b))"><XMath xml:id="S2.p6.1.m1.1"><XMApp xml:id="S2.p6.1.m1.1.6"><XMTok meaning="inner-product" role="MIDDLE" xml:id="S2.p6.1.m1.1.6.1">⁡</XMTok><XMApp xml:id="S2.p6.1.m1.1.6.2"><XMTok argclose="|" argopen="&lt;" meaning="bra" role="FENCED" xml:id="S2.p6.1.m1.1.6.2.1"/><XMTok role="UNKNOWN" xml:id="S2.p6.1.m1.1.2" font="italic">a</XMTok></XMApp><XMApp xml:id="S2.p6.1.m1.1.6.3"><XMTok argclose="&gt;" argopen="|" meaning="ket" role="FENCED" xml:id="S2.p6.1.m1.1.6.3.1"/><XMTok role="UNKNOWN" xml:id="S2.p6.1.m1.1.4" font="italic">b</XMTok></XMApp></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S2.p7">
      <p xml:id="S2.p7.1">Also <Math mode="inline" xml:id="S2.p7.1.m1" tex="&lt;a|f|b&gt;" text="inner-product@(bra@(a), f@(ket@(b)))"><XMath xml:id="S2.p7.1.m1.1"><XMApp xml:id="S2.p7.1.m1.1.8"><XMTok meaning="inner-product" role="MIDDLE" xml:id="S2.p7.1.m1.1.8.1">⁡</XMTok><XMApp xml:id="S2.p7.1.m1.1.8.2"><XMTok argclose="|" argopen="&lt;" meaning="bra" role="FENCED" xml:id="S2.p7.1.m1.1.8.2.1"/><XMTok role="UNKNOWN" xml:id="S2.p7.1.m1.1.2" font="italic">a</XMTok></XMApp><XMApp xml:id="S2.p7.1.m1.1.8.3"><XMTok role="UNKNOWN" xml:id="S2.p7.1.m1.1.4" font="italic">f</XMTok><XMApp xml:id="S2.p7.1.m1.1.8.3.1"><XMTok argclose="&gt;" argopen="|" meaning="ket" role="FENCED" xml:id="S2.p7.1.m1.1.8.3.1.1"/><XMTok role="UNKNOWN" xml:id="S2.p7.1.m1.1.6" font="italic">b</XMTok></XMApp></XMApp></XMApp></XMath></Math></p>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag close=" ">3</tag>Absolute Values</title>
    <para xml:id="S3.p1">
      <p xml:id="S3.p1.1">abs <Math mode="inline" xml:id="S3.p1.1.m1" tex="|a|" text="absolute-value@(a)"><XMath xml:id="S3.p1.1.m1.1"><XMApp xml:id="S3.p1.1.m1.1.4"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p1.1.m1.1.4.1"/><XMTok role="UNKNOWN" xml:id="S3.p1.1.m1.1.2" font="italic">a</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S3.p2">
      <p xml:id="S3.p2.1">norm <Math mode="inline" xml:id="S3.p2.1.m1" tex="||a||" text="norm@(a)"><XMath xml:id="S3.p2.1.m1.1"><XMApp xml:id="S3.p2.1.m1.1.6"><XMTok argclose="||" argopen="||" meaning="norm" role="FENCED" xml:id="S3.p2.1.m1.1.6.1"/><XMTok role="UNKNOWN" xml:id="S3.p2.1.m1.1.3" font="italic">a</XMTok></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S3.p3">
      <p xml:id="S3.p3.1">expr <Math mode="inline" xml:id="S3.p3.1.m1" tex="|a|-|b|" text="absolute-value@(a) - absolute-value@(b)"><XMath xml:id="S3.p3.1.m1.1"><XMApp xml:id="S3.p3.1.m1.1.8"><XMTok meaning="minus" role="ADDOP" xml:id="S3.p3.1.m1.1.4">-</XMTok><XMApp xml:id="S3.p3.1.m1.1.8.1"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p3.1.m1.1.8.1.1"/><XMTok role="UNKNOWN" xml:id="S3.p3.1.m1.1.2" font="italic">a</XMTok></XMApp><XMApp xml:id="S3.p3.1.m1.1.8.2"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p3.1.m1.1.8.2.1"/><XMTok role="UNKNOWN" xml:id="S3.p3.1.m1.1.6" font="italic">b</XMTok></XMApp></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S3.p4">
      <p xml:id="S3.p4.1">nested <Math mode="inline" xml:id="S3.p4.1.m1" tex="||a|-|b||" text="absolute-value@(absolute-value@(a) - absolute-value@(b))"><XMath xml:id="S3.p4.1.m1.1"><XMApp xml:id="S3.p4.1.m1.1.10"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p4.1.m1.1.10.1"/><XMApp xml:id="S3.p4.1.m1.1.10.2"><XMTok meaning="minus" role="ADDOP" xml:id="S3.p4.1.m1.1.5">-</XMTok><XMApp xml:id="S3.p4.1.m1.1.10.2.1"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p4.1.m1.1.10.2.1.1"/><XMTok role="UNKNOWN" xml:id="S3.p4.1.m1.1.3" font="italic">a</XMTok></XMApp><XMApp xml:id="S3.p4.1.m1.1.10.2.2"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p4.1.m1.1.10.2.2.1"/><XMTok role="UNKNOWN" xml:id="S3.p4.1.m1.1.7" font="italic">b</XMTok></XMApp></XMApp></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S3.p5">
      <p xml:id="S3.p5.1">non-nested <Math mode="inline" xml:id="S3.p5.1.m1" tex="a|a|+b|b|+c|c|" text="a * absolute-value@(a) + b * absolute-value@(b) + c * absolute-value@(c)"><XMath xml:id="S3.p5.1.m1.1"><XMApp xml:id="S3.p5.1.m1.1.15"><XMTok meaning="plus" role="ADDOP" xml:id="S3.p5.1.m1.1.5">+</XMTok><XMApp xml:id="S3.p5.1.m1.1.15.1"><XMTok meaning="times" role="MULOP" xml:id="S3.p5.1.m1.1.15.1.1">⁢</XMTok><XMTok role="UNKNOWN" xml:id="S3.p5.1.m1.1.1" font="italic">a</XMTok><XMApp xml:id="S3.p5.1.m1.1.15.1.2"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p5.1.m1.1.15.1.2.1"/><XMTok role="UNKNOWN" xml:id="S3.p5.1.m1.1.3" font="italic">a</XMTok></XMApp></XMApp><XMApp xml:id="S3.p5.1.m1.1.15.2"><XMTok meaning="times" role="MULOP" xml:id="S3.p5.1.m1.1.15.2.1">⁢</XMTok><XMTok role="UNKNOWN" xml:id="S3.p5.1.m1.1.6" font="italic">b</XMTok><XMApp xml:id="S3.p5.1.m1.1.15.2.2"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p5.1.m1.1.15.2.2.1"/><XMTok role="UNKNOWN" xml:id="S3.p5.1.m1.1.8" font="italic">b</XMTok></XMApp></XMApp><XMApp xml:id="S3.p5.1.m1.1.15.3"><XMTok meaning="times" role="MULOP" xml:id="S3.p5.1.m1.1.15.3.1">⁢</XMTok><XMTok role="UNKNOWN" xml:id="S3.p5.1.m1.1.11" font="italic">c</XMTok><XMApp xml:id="S3.p5.1.m1.1.15.3.2"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.p5.1.m1.1.15.3.2.1"/><XMTok role="UNKNOWN" xml:id="S3.p5.1.m1.1.13" font="italic">c</XMTok></XMApp></XMApp></XMApp></XMath></Math></p>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag close=" ">4</tag>Mixtures that look like QM</title>
    <para xml:id="S4.p1">
      <p xml:id="S4.p1.1">abs and rel: <Math mode="inline" xml:id="S4.p1.1.m1" tex="a&lt;c|c|" text="a less c * absolute-value@(c)"><XMath xml:id="S4.p1.1.m1.1"><XMApp xml:id="S4.p1.1.m1.1.7"><XMTok meaning="less-than" role="RELOP" xml:id="S4.p1.1.m1.1.2">&lt;</XMTok><XMTok role="UNKNOWN" xml:id="S4.p1.1.m1.1.1" font="italic">a</XMTok><XMApp xml:id="S4.p1.1.m1.1.7.1"><XMTok meaning="times" role="MULOP" xml:id="S4.p1.1.m1.1.7.1.1">⁢</XMTok><XMTok role="UNKNOWN" xml:id="S4.p1.1.m1.1.3" font="italic">c</XMTok><XMApp xml:id="S4.p1.1.m1.1.7.1.2"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S4.p1.1.m1.1.7.1.2.1"/><XMTok role="UNKNOWN" xml:id="S4.p1.1.m1.1.5" font="italic">c</XMTok></XMApp></XMApp></XMApp></XMath></Math></p>
    </para>
    <para xml:id="S4.p2">
      <p xml:id="S4.p2.1">worse <Math mode="inline" xml:id="S4.p2.1.m1" tex="|a|&lt;\min\left|z\pm\left(z^{{2}}-1\right)^{{{1/2}}}\right|" text="absolute-value@(a) less minimum@(absolute-value@(z plus-or-minus (z ^ 2 - 1) ^ (1 / 2)))"><XMath xml:id="S4.p2.1.m1.1"><XMApp xml:id="S4.p2.1.m1.1.17"><XMTok meaning="less-than" role="RELOP" xml:id="S4.p2.1.m1.1.4">&lt;</XMTok><XMApp xml:id="S4.p2.1.m1.1.17.1"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S4.p2.1.m1.1.17.1.1"/><XMTok role="UNKNOWN" xml:id="S4.p2.1.m1.1.2" font="italic">a</XMTok></XMApp><XMApp xml:id="S4.p2.1.m1.1.17.2"><XMTok meaning="minimum" role="LIMITOP" scriptpos="post" xml:id="S4.p2.1.m1.1.5">min</XMTok><XMApp xml:id="S4.p2.1.m1.1.17.2.1"><XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S4.p2.1.m1.1.17.2.1.1"/><XMApp xml:id="S4.p2.1.m1.1.17.2.1.2"><XMTok meaning="plus-or-minus" name="pm" role="ADDOP" xml:id="S4.p2.1.m1.1.8">±</XMTok><XMTok role="UNKNOWN" xml:id="S4.p2.1.m1.1.7" font="italic">z</XMTok><XMApp xml:id="S4.p2.1.m1.1.17.2.1.2.1"><XMTok role="SUPERSCRIPTOP" scriptpos="post3" xml:id="S4.p2.1.m1.1.17.2.1.2.1.1"/><XMApp close=")" open="(" xml:id="S4.p2.1.m1.1.17.2.1.2.1.2"><XMTok meaning="minus" role="ADDOP" xml:id="S4.p2.1.m1.1.12">-</XMTok><XMApp xml:id="S4.p2.1.m1.1.17.2.1.2.1.2.1"><XMTok role="SUPERSCRIPTOP" scriptpos="post4" xml:id="S4.p2.1.m1.1.17.2.1.2.1.2.1.1"/><XMTok role="UNKNOWN" xml:id="S4.p2.1.m1.1.10" font="italic">z</XMTok><XMTok meaning="2" role="NUMBER" xml:id="S4.p2.1.m1.1.11.1">2</XMTok></XMApp><XMTok meaning="1" role="NUMBER" xml:id="S4.p2.1.m1.1.13">1</XMTok></XMApp><XMApp xml:id="S4.p2.1.m1.1.15.1"><XMTok mathstyle="inline" meaning="divide" role="MULOP" xml:id="S4.p2.1.m1.1.15.1.2">/</XMTok><XMTok meaning="1" role="NUMBER" xml:id="S4.p2.1.m1.1.15.1.1">1</XMTok><XMTok meaning="2" role="NUMBER" xml:id="S4.p2.1.m1.1.15.1.3">2</XMTok></XMApp></XMApp></XMApp></XMApp></XMApp></XMApp></XMath></Math></p>
    </para>
  </section>
</document>
