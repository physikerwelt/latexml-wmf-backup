  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" xml:id="S0.Ex1.m1" tex="\{a\}" text="set@(a)">
        <XMath xml:id="S0.Ex1.m1.1">
          <XMApp xml:id="S0.Ex1.m1.1.4">
            <XMTok argclose="}" argopen="{" meaning="set" role="FENCED" xml:id="S0.Ex1.m1.1.4.1"/>
            <XMTok role="ID" xml:id="S0.Ex1.m1.1.2" font="italic">a</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" xml:id="S0.Ex2.m1" tex="\{a:a\in b\}" text="conditional-set@(a, a element-of b)">
        <XMath xml:id="S0.Ex2.m1.1">
          <XMApp xml:id="S0.Ex2.m1.1.8">
            <XMTok argclose="}" argopen="{" meaning="conditional-set" role="FENCED" separators=":" xml:id="S0.Ex2.m1.1.8.1"/>
            <XMTok role="ID" xml:id="S0.Ex2.m1.1.2" font="italic">a</XMTok>
            <XMApp xml:id="S0.Ex2.m1.1.8.2">
              <XMTok meaning="element-of" name="in" role="RELOP" xml:id="S0.Ex2.m1.1.5">∈</XMTok>
              <XMTok role="ID" xml:id="S0.Ex2.m1.1.4" font="italic">a</XMTok>
              <XMTok role="ID" xml:id="S0.Ex2.m1.1.6" font="italic">b</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex3">
      <Math mode="display" xml:id="S0.Ex3.m1" tex="\{a|a\in b\}" text="conditional-set@(a, a element-of b)">
        <XMath xml:id="S0.Ex3.m1.1">
          <XMApp xml:id="S0.Ex3.m1.1.8">
            <XMTok argclose="}" argopen="{" meaning="conditional-set" role="FENCED" separators="|" xml:id="S0.Ex3.m1.1.8.1"/>
            <XMTok role="ID" xml:id="S0.Ex3.m1.1.2" font="italic">a</XMTok>
            <XMApp xml:id="S0.Ex3.m1.1.8.2">
              <XMTok meaning="element-of" name="in" role="RELOP" xml:id="S0.Ex3.m1.1.5">∈</XMTok>
              <XMTok role="ID" xml:id="S0.Ex3.m1.1.4" font="italic">a</XMTok>
              <XMTok role="ID" xml:id="S0.Ex3.m1.1.6" font="italic">b</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex4">
      <Math mode="display" xml:id="S0.Ex4.m1" tex="\{a|a\in b\}_{{c=0}}" text="(conditional-set@(a, a element-of b)) _ (c = 0)">
        <XMath xml:id="S0.Ex4.m1.1">
          <XMApp xml:id="S0.Ex4.m1.1.9">
            <XMTok role="SUBSCRIPTOP" scriptpos="post2" xml:id="S0.Ex4.m1.1.9.1"/>
            <XMApp xml:id="S0.Ex4.m1.1.9.2">
              <XMTok argclose="}" argopen="{" meaning="conditional-set" role="FENCED" separators="|" xml:id="S0.Ex4.m1.1.9.2.1"/>
              <XMTok role="ID" xml:id="S0.Ex4.m1.1.2" font="italic">a</XMTok>
              <XMApp xml:id="S0.Ex4.m1.1.9.2.2">
                <XMTok meaning="element-of" name="in" role="RELOP" xml:id="S0.Ex4.m1.1.5">∈</XMTok>
                <XMTok role="ID" xml:id="S0.Ex4.m1.1.4" font="italic">a</XMTok>
                <XMTok role="ID" xml:id="S0.Ex4.m1.1.6" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
            <XMApp xml:id="S0.Ex4.m1.1.8.1">
              <XMTok meaning="equals" role="RELOP" xml:id="S0.Ex4.m1.1.8.1.2">=</XMTok>
              <XMTok role="ID" xml:id="S0.Ex4.m1.1.8.1.1" font="italic">c</XMTok>
              <XMTok meaning="0" role="NUMBER" xml:id="S0.Ex4.m1.1.8.1.3">0</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
