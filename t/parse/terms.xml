<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section refnum="1" xml:id="S1">
    <title><tag close=" ">1</tag>Basic math</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" xml:id="S1.Ex1.m1" tex="a+b+c-d+e" text="((a + b + c) - d) + e">
          <XMath xml:id="S1.Ex1.m1.1">
            <XMApp xml:id="S1.Ex1.m1.1.10">
              <XMTok meaning="plus" role="ADDOP" xml:id="S1.Ex1.m1.1.8">+</XMTok>
              <XMApp xml:id="S1.Ex1.m1.1.10.1">
                <XMTok meaning="minus" role="ADDOP" xml:id="S1.Ex1.m1.1.6">-</XMTok>
                <XMApp xml:id="S1.Ex1.m1.1.10.1.1">
                  <XMTok meaning="plus" role="ADDOP" xml:id="S1.Ex1.m1.1.2">+</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex1.m1.1.1" font="italic">a</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex1.m1.1.3" font="italic">b</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex1.m1.1.5" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S1.Ex1.m1.1.7" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" xml:id="S1.Ex1.m1.1.9" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" xml:id="S1.Ex2.m1" tex="a+bc-de" text="(a + b * c) - d * e">
          <XMath xml:id="S1.Ex2.m1.1">
            <XMApp xml:id="S1.Ex2.m1.1.8">
              <XMTok meaning="minus" role="ADDOP" xml:id="S1.Ex2.m1.1.5">-</XMTok>
              <XMApp xml:id="S1.Ex2.m1.1.8.1">
                <XMTok meaning="plus" role="ADDOP" xml:id="S1.Ex2.m1.1.2">+</XMTok>
                <XMTok role="ID" xml:id="S1.Ex2.m1.1.1" font="italic">a</XMTok>
                <XMApp xml:id="S1.Ex2.m1.1.8.1.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex2.m1.1.8.1.1.1">⁢</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex2.m1.1.3" font="italic">b</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex2.m1.1.4" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp xml:id="S1.Ex2.m1.1.8.2">
                <XMTok meaning="times" role="MULOP" xml:id="S1.Ex2.m1.1.8.2.1">⁢</XMTok>
                <XMTok role="ID" xml:id="S1.Ex2.m1.1.6" font="italic">d</XMTok>
                <XMTok role="ID" xml:id="S1.Ex2.m1.1.7" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" xml:id="S1.Ex3.m1" tex="abc/de" text="((a * b * c) / d) * e">
          <XMath xml:id="S1.Ex3.m1.1">
            <XMApp xml:id="S1.Ex3.m1.1.7">
              <XMTok meaning="times" role="MULOP" xml:id="S1.Ex3.m1.1.7.1">⁢</XMTok>
              <XMApp xml:id="S1.Ex3.m1.1.7.2">
                <XMTok mathstyle="inline" meaning="divide" role="MULOP" xml:id="S1.Ex3.m1.1.4">/</XMTok>
                <XMApp xml:id="S1.Ex3.m1.1.7.2.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex3.m1.1.7.2.1.1">⁢</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex3.m1.1.1" font="italic">a</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex3.m1.1.2" font="italic">b</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex3.m1.1.3" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S1.Ex3.m1.1.5" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" xml:id="S1.Ex3.m1.1.6" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" xml:id="S1.Ex4.m1" tex="a\times b\times c/d\times e" text="((a * b * c) / d) * e">
          <XMath xml:id="S1.Ex4.m1.1">
            <XMApp xml:id="S1.Ex4.m1.1.10">
              <XMTok meaning="times" role="MULOP" xml:id="S1.Ex4.m1.1.8">×</XMTok>
              <XMApp xml:id="S1.Ex4.m1.1.10.1">
                <XMTok mathstyle="inline" meaning="divide" role="MULOP" xml:id="S1.Ex4.m1.1.6">/</XMTok>
                <XMApp xml:id="S1.Ex4.m1.1.10.1.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex4.m1.1.4">×</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex4.m1.1.1" font="italic">a</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex4.m1.1.3" font="italic">b</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex4.m1.1.5" font="italic">c</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S1.Ex4.m1.1.7" font="italic">d</XMTok>
              </XMApp>
              <XMTok role="ID" xml:id="S1.Ex4.m1.1.9" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <paragraph xml:id="S1.SS0.SSS0.P1">
      <title>N-ary special cases</title>
      <para xml:id="S1.SS0.SSS0.P1.p1">
        <equation xml:id="S1.Ex5">
          <Math mode="display" xml:id="S1.Ex5.m1" tex="a\times b\times c\times d" text="a * b * c * d">
            <XMath xml:id="S1.Ex5.m1.1">
              <XMApp xml:id="S1.Ex5.m1.1.8">
                <XMTok meaning="times" role="MULOP" xml:id="S1.Ex5.m1.1.6">×</XMTok>
                <XMTok role="ID" xml:id="S1.Ex5.m1.1.1" font="italic">a</XMTok>
                <XMTok role="ID" xml:id="S1.Ex5.m1.1.3" font="italic">b</XMTok>
                <XMTok role="ID" xml:id="S1.Ex5.m1.1.5" font="italic">c</XMTok>
                <XMTok role="ID" xml:id="S1.Ex5.m1.1.7" font="italic">d</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex6">
          <Math mode="display" xml:id="S1.Ex6.m1" tex="(a\times b)\times(c\times d)" text="a * b * c * d">
            <XMath xml:id="S1.Ex6.m1.1">
              <XMApp xml:id="S1.Ex6.m1.1.12">
                <XMTok meaning="times" role="MULOP" xml:id="S1.Ex6.m1.1.6">×</XMTok>
                <XMApp close=")" open="(" xml:id="S1.Ex6.m1.1.12.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex6.m1.1.3">×</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex6.m1.1.2" font="italic">a</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex6.m1.1.4" font="italic">b</XMTok>
                </XMApp>
                <XMApp close=")" open="(" xml:id="S1.Ex6.m1.1.12.2">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex6.m1.1.9">×</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex6.m1.1.8" font="italic">c</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex6.m1.1.10" font="italic">d</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex7">
          <Math mode="display" xml:id="S1.Ex7.m1" tex="((a\times b)\times(c\times d))" text="a * b * c * d">
            <XMath xml:id="S1.Ex7.m1.1">
              <XMApp close=")" open="(" xml:id="S1.Ex7.m1.1.14">
                <XMTok meaning="times" role="MULOP" xml:id="S1.Ex7.m1.1.7">×</XMTok>
                <XMApp close=")" open="(" xml:id="S1.Ex7.m1.1.14.1">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex7.m1.1.4">×</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex7.m1.1.3" font="italic">a</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex7.m1.1.5" font="italic">b</XMTok>
                </XMApp>
                <XMApp close=")" open="(" xml:id="S1.Ex7.m1.1.14.2">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex7.m1.1.10">×</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex7.m1.1.9" font="italic">c</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex7.m1.1.11" font="italic">d</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
      <para xml:id="S1.SS0.SSS0.P1.p2">
        <equation xml:id="S1.Ex8">
          <Math mode="display" xml:id="S1.Ex8.m1" tex="a/b/c" text="a / b / c">
            <XMath xml:id="S1.Ex8.m1.1">
              <XMApp xml:id="S1.Ex8.m1.1.6">
                <XMTok mathstyle="inline" meaning="divide" role="MULOP" xml:id="S1.Ex8.m1.1.4">/</XMTok>
                <XMTok role="ID" xml:id="S1.Ex8.m1.1.1" font="italic">a</XMTok>
                <XMTok role="ID" xml:id="S1.Ex8.m1.1.3" font="italic">b</XMTok>
                <XMTok role="ID" xml:id="S1.Ex8.m1.1.5" font="italic">c</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex9">
          <Math mode="display" xml:id="S1.Ex9.m1" tex="\frac{a}{b}/c" text="a / b / c">
            <XMath xml:id="S1.Ex9.m1.1">
              <XMApp xml:id="S1.Ex9.m1.1.4">
                <XMTok mathstyle="inline" meaning="divide" role="MULOP" xml:id="S1.Ex9.m1.1.2">/</XMTok>
                <XMApp xml:id="S1.Ex9.m1.1.1">
                  <XMTok mathstyle="display" meaning="divide" role="MULOP" xml:id="S1.Ex9.m1.1.1.1"/>
                  <XMTok role="ID" xml:id="S1.Ex9.m1.1.1.2" font="italic">a</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex9.m1.1.1.3" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="ID" xml:id="S1.Ex9.m1.1.3" font="italic">c</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex10">
          <Math mode="display" xml:id="S1.Ex10.m1" tex="a/\frac{b}{c}" text="a / b / c">
            <XMath xml:id="S1.Ex10.m1.1">
              <XMApp xml:id="S1.Ex10.m1.1.4">
                <XMTok mathstyle="inline" meaning="divide" role="MULOP" xml:id="S1.Ex10.m1.1.2">/</XMTok>
                <XMTok role="ID" xml:id="S1.Ex10.m1.1.1" font="italic">a</XMTok>
                <XMApp xml:id="S1.Ex10.m1.1.3">
                  <XMTok mathstyle="display" meaning="divide" role="MULOP" xml:id="S1.Ex10.m1.1.3.1"/>
                  <XMTok role="ID" xml:id="S1.Ex10.m1.1.3.2" font="italic">b</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex10.m1.1.3.3" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
      <para xml:id="S1.SS0.SSS0.P1.p3">
        <equation xml:id="S1.Ex11">
          <Math mode="display" xml:id="S1.Ex11.m1" tex="a\times\framebox{}{}{$b\times c$}" text="a * b * c">
            <XMath xml:id="S1.Ex11.m1.1">
              <XMApp xml:id="S1.Ex11.m1.1.4">
                <XMTok meaning="times" role="MULOP" xml:id="S1.Ex11.m1.1.2">×</XMTok>
                <XMTok role="ID" xml:id="S1.Ex11.m1.1.1" font="italic">a</XMTok>
                <XMApp enclose="box" xml:id="S1.Ex11.m1.1.3">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.Ex11.m1.1.3.2">×</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex11.m1.1.3.1" font="italic">b</XMTok>
                  <XMTok role="ID" xml:id="S1.Ex11.m1.1.3.3" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
    </paragraph>
  </section>
  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Evaluate At</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex12">
        <Math mode="display" xml:id="S2.Ex12.m1" tex="a|_{\infty}" text="evaluated-at@(a, infinity)">
          <XMath xml:id="S2.Ex12.m1.1">
            <XMApp xml:id="S2.Ex12.m1.1.4">
              <XMTok meaning="evaluated-at" xml:id="S2.Ex12.m1.1.4.1"/>
              <XMTok role="ID" xml:id="S2.Ex12.m1.1.1" font="italic">a</XMTok>
              <XMTok meaning="infinity" name="infty" role="ID" xml:id="S2.Ex12.m1.1.3.1">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex13">
        <Math mode="display" xml:id="S2.Ex13.m1" tex="ab|_{{a=0}}cd|_{{c=0}}e" text="evaluated-at@(evaluated-at@(a * b, a = 0) * c * d, c = 0) * e">
          <XMath xml:id="S2.Ex13.m1.1">
            <XMApp xml:id="S2.Ex13.m1.1.10">
              <XMTok meaning="times" role="MULOP" xml:id="S2.Ex13.m1.1.10.1">⁢</XMTok>
              <XMApp xml:id="S2.Ex13.m1.1.10.2">
                <XMTok meaning="evaluated-at" xml:id="S2.Ex13.m1.1.10.2.1"/>
                <XMApp xml:id="S2.Ex13.m1.1.10.2.2">
                  <XMTok meaning="times" role="MULOP" xml:id="S2.Ex13.m1.1.10.2.2.1">⁢</XMTok>
                  <XMApp xml:id="S2.Ex13.m1.1.10.2.2.2">
                    <XMTok meaning="evaluated-at" xml:id="S2.Ex13.m1.1.10.2.2.2.1"/>
                    <XMApp xml:id="S2.Ex13.m1.1.10.2.2.2.2">
                      <XMTok meaning="times" role="MULOP" xml:id="S2.Ex13.m1.1.10.2.2.2.2.1">⁢</XMTok>
                      <XMTok role="ID" xml:id="S2.Ex13.m1.1.1" font="italic">a</XMTok>
                      <XMTok role="ID" xml:id="S2.Ex13.m1.1.2" font="italic">b</XMTok>
                    </XMApp>
                    <XMApp xml:id="S2.Ex13.m1.1.4.1">
                      <XMTok meaning="equals" role="RELOP" xml:id="S2.Ex13.m1.1.4.1.2">=</XMTok>
                      <XMTok role="ID" xml:id="S2.Ex13.m1.1.4.1.1" font="italic">a</XMTok>
                      <XMTok meaning="0" role="NUMBER" xml:id="S2.Ex13.m1.1.4.1.3">0</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMTok role="ID" xml:id="S2.Ex13.m1.1.5" font="italic">c</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex13.m1.1.6" font="italic">d</XMTok>
                </XMApp>
                <XMApp xml:id="S2.Ex13.m1.1.8.1">
                  <XMTok meaning="equals" role="RELOP" xml:id="S2.Ex13.m1.1.8.1.2">=</XMTok>
                  <XMTok role="ID" xml:id="S2.Ex13.m1.1.8.1.1" font="italic">c</XMTok>
                  <XMTok meaning="0" role="NUMBER" xml:id="S2.Ex13.m1.1.8.1.3">0</XMTok>
                </XMApp>
              </XMApp>
              <XMTok role="ID" xml:id="S2.Ex13.m1.1.9" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title><tag close=" ">3</tag>Absolute values</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex14">
        <Math mode="display" xml:id="S3.Ex14.m1" tex="|a|" text="absolute-value@(a)">
          <XMath xml:id="S3.Ex14.m1.1">
            <XMApp xml:id="S3.Ex14.m1.1.4">
              <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.Ex14.m1.1.4.1"/>
              <XMTok role="ID" xml:id="S3.Ex14.m1.1.2" font="italic">a</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex15">
        <Math mode="display" xml:id="S3.Ex15.m1" tex="||a||" text="norm@(a)">
          <XMath xml:id="S3.Ex15.m1.1">
            <XMApp xml:id="S3.Ex15.m1.1.6">
              <XMTok argclose="||" argopen="||" meaning="norm" role="FENCED" xml:id="S3.Ex15.m1.1.6.1"/>
              <XMTok role="ID" xml:id="S3.Ex15.m1.1.3" font="italic">a</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex16">
        <Math mode="display" xml:id="S3.Ex16.m1" tex="|a|_{\infty}" text="(absolute-value@(a)) _ infinity">
          <XMath xml:id="S3.Ex16.m1.1">
            <XMApp xml:id="S3.Ex16.m1.1.5">
              <XMTok role="SUBSCRIPTOP" scriptpos="post2" xml:id="S3.Ex16.m1.1.5.1"/>
              <XMApp xml:id="S3.Ex16.m1.1.5.2">
                <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.Ex16.m1.1.5.2.1"/>
                <XMTok role="ID" xml:id="S3.Ex16.m1.1.2" font="italic">a</XMTok>
              </XMApp>
              <XMTok meaning="infinity" name="infty" role="ID" xml:id="S3.Ex16.m1.1.4.1">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex17">
        <Math mode="display" xml:id="S3.Ex17.m1" tex="|a+b|" text="absolute-value@(a + b)">
          <XMath xml:id="S3.Ex17.m1.1">
            <XMApp xml:id="S3.Ex17.m1.1.6">
              <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.Ex17.m1.1.6.1"/>
              <XMApp xml:id="S3.Ex17.m1.1.6.2">
                <XMTok meaning="plus" role="ADDOP" xml:id="S3.Ex17.m1.1.3">+</XMTok>
                <XMTok role="ID" xml:id="S3.Ex17.m1.1.2" font="italic">a</XMTok>
                <XMTok role="ID" xml:id="S3.Ex17.m1.1.4" font="italic">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex18">
        <Math mode="display" xml:id="S3.Ex18.m1" tex="|a-|b||" text="absolute-value@(a - absolute-value@(b))">
          <XMath xml:id="S3.Ex18.m1.1">
            <XMApp xml:id="S3.Ex18.m1.1.8">
              <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.Ex18.m1.1.8.1"/>
              <XMApp xml:id="S3.Ex18.m1.1.8.2">
                <XMTok meaning="minus" role="ADDOP" xml:id="S3.Ex18.m1.1.3">-</XMTok>
                <XMTok role="ID" xml:id="S3.Ex18.m1.1.2" font="italic">a</XMTok>
                <XMApp xml:id="S3.Ex18.m1.1.8.2.1">
                  <XMTok argclose="|" argopen="|" meaning="absolute-value" role="FENCED" xml:id="S3.Ex18.m1.1.8.2.1.1"/>
                  <XMTok role="ID" xml:id="S3.Ex18.m1.1.5" font="italic">b</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="4" xml:id="S4">
    <title><tag close=" ">4</tag>Failures w/o Backtracking</title>
    <para xml:id="S4.p1">
      <equation xml:id="S4.Ex19">
        <Math mode="display" xml:id="S4.Ex19.m1" tex="||a|-|b||" text="absolute-value@(absolute-value@(a) - absolute-value@(b))">
          <XMath xml:id="S4.Ex19.m1.1">
