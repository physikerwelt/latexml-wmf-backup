                        <XMTok role="UNKNOWN" xml:id="S1.E1.m1.1.3.2.1" font="italic">d</XMTok>
                        <XMTok meaning="maximum" role="LIMITOP" scriptpos="post" xml:id="S1.E1.m1.1.3.2.2.1">max</XMTok>
                      </XMApp>
                      <XMTok meaning="2" role="NUMBER" xml:id="S1.E1.m1.1.3.2.3.1">2</XMTok>
                    </XMApp>
                    <XMApp xml:id="S1.E1.m1.1.3.2.9">
                      <XMTok role="SUPERSCRIPTOP" scriptpos="post4" xml:id="S1.E1.m1.1.3.2.9.1"/>
                      <XMApp xml:id="S1.E1.m1.1.3.2.9.2">
                        <XMTok role="SUBSCRIPTOP" scriptpos="post4" xml:id="S1.E1.m1.1.3.2.9.2.1"/>
                        <XMTok role="UNKNOWN" xml:id="S1.E1.m1.1.3.2.5" font="italic">d</XMTok>
                        <XMTok meaning="minimum" role="LIMITOP" scriptpos="post" xml:id="S1.E1.m1.1.3.2.6.1">min</XMTok>
                      </XMApp>
                      <XMTok meaning="2" role="NUMBER" xml:id="S1.E1.m1.1.3.2.7.1">2</XMTok>
                    </XMApp>
                  </XMApp>
