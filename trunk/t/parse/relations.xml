<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <section refnum="1" xml:id="S1">
    <title>Grouping in multi-relations</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a=b=c=d=e" text="a = b = c = d = e">
          <XMath>
            <XMApp>
              <XMTok name="MultiRelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="a=b,c=d" text="Formulae@(a = b, c = d)">
          <XMath>
            <XMApp>
              <XMTok name="Formulae" separators=","/>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" tex="a=b,c,d,e" text="a = Collection@(b, c, d, e)">
          <XMath>
            <XMApp>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok name="Collection" role="FENCED" separators=",,,"/>
                <XMTok role="ID" font="italic">b</XMTok>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" tex="a=b,c,d=e" text="Formulae@(a = Collection@(b, c), d = e)">
          <XMath>
            <XMApp>
              <XMTok name="Formulae" separators=","/>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok name="Collection" role="FENCED" separators=","/>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex5">
        <Math mode="display" tex="a=b&gt;c\le d=e" text="a = b less c less-eq d = e">
          <XMath>
            <XMApp>
              <XMTok name="MultiRelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok meaning="greater" role="RELOP">&gt;</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="less-eq" name="le" role="RELOP">≤</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <paragraph xml:id="S1.SS0.SSS0.P1">
      <title>But period is somewhat different!</title>
      <para xml:id="S1.SS0.SSS0.P1.p1">
        <equation xml:id="S1.Ex6">
          <Math mode="display" tex="a=b,c.d=e" text="Formulae@(a = Collection@(b, c), d = e)">
            <XMath>
              <XMApp>
                <XMTok name="Formulae" separators="."/>
                <XMApp>
                  <XMTok meaning="eq" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMApp>
                    <XMTok name="Collection" role="FENCED" separators=","/>
                    <XMTok role="ID" font="italic">b</XMTok>
                    <XMTok role="ID" font="italic">c</XMTok>
                  </XMApp>
                </XMApp>
                <XMApp>
                  <XMTok meaning="eq" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex7">
          <Math mode="display" tex="a=b.c,d=e" text="Formulae@(a = b, Collection@(c, d) = e)">
            <XMath>
              <XMApp>
                <XMTok name="Formulae" separators="."/>
                <XMApp>
                  <XMTok meaning="eq" role="RELOP">=</XMTok>
                  <XMTok role="ID" font="italic">a</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="eq" role="RELOP">=</XMTok>
                  <XMApp>
                    <XMTok name="Collection" role="FENCED" separators=","/>
                    <XMTok role="ID" font="italic">c</XMTok>
                    <XMTok role="ID" font="italic">d</XMTok>
                  </XMApp>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
    </paragraph>
  </section>
  <section refnum="2" xml:id="S2">
    <title>Arrows as meta-relation</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex8">
        <Math mode="display" tex="a=b\rightarrow c\le d=e" text="a = b rightarrow c less-eq d = e">
          <XMath>
            <XMApp>
              <XMTok name="MultiRelation"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">b</XMTok>
              <XMTok name="rightarrow" role="ARROW">→</XMTok>
              <XMTok role="ID" font="italic">c</XMTok>
              <XMTok meaning="less-eq" name="le" role="RELOP">≤</XMTok>
              <XMTok role="ID" font="italic">d</XMTok>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex9">
        <Math mode="display" tex="a=b,c\rightarrow d=e" text="Formulae@(a = b, c rightarrow d = e)">
          <XMath>
            <XMApp>
              <XMTok name="Formulae" separators=","/>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok name="MultiRelation"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok name="rightarrow" role="ARROW">→</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex10">
        <Math mode="display" tex="a=b\leftrightarrow c\le d=e" text="a = b leftrightarrow c less-eq d = e">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMTok role="ID" font="italic">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok name="MultiRelation"/>
                <XMTok role="ID" font="italic">c</XMTok>
                <XMTok meaning="less-eq" name="le" role="RELOP">≤</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex11">
        <Math mode="display" tex="a=b,c\leftrightarrow d=e" text="a = Collection@(b, c) leftrightarrow d = e">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">a</XMTok>
                <XMApp>
                  <XMTok name="Collection" role="FENCED" separators=","/>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="eq" role="RELOP">=</XMTok>
                <XMTok role="ID" font="italic">d</XMTok>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section refnum="3" xml:id="S3">
    <title>Other Meta relations</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex12">
        <Math mode="display" tex="a(&lt;e)" text="Annotated@(a, Empty less e)">
          <XMath>
            <XMApp>
              <XMTok name="Annotated"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="(" close=")">
                <XMTok meaning="less" role="RELOP">&lt;</XMTok>
                <XMTok name="Empty"/>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex13">
        <Math mode="display" tex="a;(&lt;e)" text="Annotated@(a, Empty less e)">
          <XMath>
            <XMApp>
              <XMTok name="Annotated"/>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp open="(" close=")">
                <XMTok meaning="less" role="RELOP">&lt;</XMTok>
                <XMTok name="Empty"/>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex14">
        <Math mode="display" tex="a=b+c+d(&lt;e)" text="a = Annotated@(b + c + d, Empty less e)">
          <XMath>
            <XMApp>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok name="Annotated"/>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMApp open="(" close=")">
                  <XMTok meaning="less" role="RELOP">&lt;</XMTok>
                  <XMTok name="Empty"/>
                  <XMTok role="ID" font="italic">e</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex15">
        <Math mode="display" tex="a=b+c+d\mod e" text="a = mod@(b + c + d, e)">
          <XMath>
            <XMApp>
              <XMTok meaning="eq" role="RELOP">=</XMTok>
              <XMTok role="ID" font="italic">a</XMTok>
              <XMApp>
                <XMTok role="MODIFIEROP">mod</XMTok>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok role="ID" font="italic">b</XMTok>
                  <XMTok role="ID" font="italic">c</XMTok>
                  <XMTok role="ID" font="italic">d</XMTok>
                </XMApp>
                <XMTok role="ID" font="italic">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
</document>
